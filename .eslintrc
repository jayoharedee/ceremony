{
    "extends": ["eslint:recommended", "prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn",
      "no-dupe-keys": "error",
      "consistent-return": "error",
      "no-multi-spaces": "error",
      "semi": "off"
    }
  }